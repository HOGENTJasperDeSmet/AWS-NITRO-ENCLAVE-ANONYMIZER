{
    "Version": "2012-10-17",
    "Id": "key-consolepolicy-3",
    "Statement": [
        {
            "Sid": "Enable IAM User Permissions",
            "Effect": "Allow",
            "Principal": {
                "AWS": "arn:aws:iam::859548415772:root"
            },
            "Action": "kms:*",
            "Resource": "*"
        },
        {
            "Sid": "Enable Cloud9 Environment to Encrypt",
            "Effect": "Allow",
            "Principal": {
                "AWS": "arn:aws:sts::859548415772:assumed-role/AWSCloud9SSMAccessRole/i-02db041727b56064f"
            },
            "Action": "kms:Encrypt",
            "Resource": "*"
        },
        {
            "Sid": "Enable Cloud9 Environment to Decrypt",
            "Effect": "Allow",
            "Principal": {
                "AWS": "arn:aws:sts::859548415772:assumed-role/AWSCloud9SSMAccessRole/i-02db041727b56064f"
            },
            "Action": "kms:Decrypt",
            "Resource": "*",
            "Condition": {
                "StringEqualsIgnoreCase": {
                    "kms:RecipientAttestation:PCR0": "7703c936bf3af3115d703e72ca2ffe07feb3338062a4dcfe2289fd1e9e9cb4fb1c05c83e68fa0caee650892d32deb14b"
                }
            }
        }
 
    ]
}